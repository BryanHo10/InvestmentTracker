{
	"name": "StockUp",
	"version": "1.0",
	"manifest_version": 2,
	"description": "This displays real-time stock prices and personal impact",
	"browser_action": {
		"default_icon":"Stock.png"
	},
	"content_scripts": [
		{
			"matches":["<all_urls>"],
			"js": ["content.js"]
		}
	],
	"background": {
		"scripts": ["background.js"] 
	}

}